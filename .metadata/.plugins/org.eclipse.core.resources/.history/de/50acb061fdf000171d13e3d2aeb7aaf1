package com.reverse;

public class ReverseNumber {
	public boolean isPalindrome(int x) {
		int y = x;
        while(x%10!=0){
        	y= y%10;
        	System.out.println(y);
        }
		return false;
    }
	public static void main(String[] args) {
		System.out.println(new ReverseNumber().isPalindrome(123));
	}
}
