package com.reverse;

public class ReverseNumber {
	public boolean isPalindrome(int x) {
        while(x!=0){
        	System.out.println(x%10);
        	x=x/10;
        }
		return false;
    }
	public static void main(String[] args) {
		System.out.println(new ReverseNumber().isPalindrome(12345));
	}
}
