package com.reverse;

public class ReverseNumber {
	public boolean isPalindrome(int x) {
		int length = 0;
		int revInt = 0;
		int i = 0;
        while(x!=0){
        	x=x/10;
        	length++;
        } i = length;
        while(x!=0){
        	revInt = (x%10) * (int)Math.pow(10, i);
        	x=x/10;
        	i--;
        }
        System.out.println(revInt);
        System.out.println((int)Math.pow(10, length));
		return false;
    }
	public static void main(String[] args) {
		System.out.println(new ReverseNumber().isPalindrome(12345));
	}
}
